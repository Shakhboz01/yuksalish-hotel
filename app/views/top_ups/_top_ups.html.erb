<h4>Приходы</h4>
<table class="table table-hover shadow table-striped">
  <thead>
    <tr>
      <th>Имя</th>
      <th>Цена<br><%= num_to_usd total_price %> </th>
      <th>Оплата оформлено в</th>
      <th>гость пришел в</th>
      <th>Номер комнаты</th>
      <th>Коммент</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% top_ups.each do |top_up| %>
      <% top_up.booking.guest_infos.each do |guest_info| %>
        <tr>
          <td><%= guest_info.name %></td>
          <td><%= top_up.price.to_f / top_up.booking.guest_infos.count %></td>
          <td><%= strf_datetime top_up.created_at %></td>
          <td><%= strf_datetime guest_info.created_at %></td>
          <td><%= top_up.booking.home.number %></td>
          <td><%= top_up.comment %></td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>

<h4>Приходы</h4>
<table class="table table-hover shadow table-striped">
  <thead>
    <tr>
      <th>Имя</th>
      <th>Цена<br><%= num_to_usd total_price %> </th>
      <th>Оформлено в</th>
      <th>Номер комнаты</th>
      <th>Коммент</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% top_ups.each do |top_up| %>
      <tr>
        <td><%= top_up.booking.guest_infos.first.name %></td>
        <td><%= num_to_usd top_up.price %></td>
        <td><%= strf_datetime top_up.created_at %></td>
        <td><%= top_up.booking.home.number %></td>
        <td><%= top_up.comment %></td>
      </tr>
    <% end %>
  </tbody>
</table>
